<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <act_window id="setting_aid_tax"
                name="稅報資料維護"
                res_model="setting.aid.tax"
                view_mode="form"/>

    <record id="setting_aid_tax_form" model="ir.ui.view">
        <field name="name">setting_aid_tax_form</field>
        <field name="model">setting.aid.tax</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <h1>用戶稅報資料維護</h1>
                    <group col="4">
                        <group colspan="4" col="6">
                            <field name="pyrecedcap"/>
                            <field name="tyrecedcap" readonly="1"/>
                            <field name="divintratio"/>
                            <field name="cashcapincre"/>
                            <field name="hrcapincre"/>
                            <field name="nescucapincre"/>
                            <field name="othercapincre"/>
                            <field name="tycapdecre"/>
                        </group>
                        <group colspan="4" col="6">
                            <field name="leglsupcapincre"/>
                            <field name="capsupcapincre"/>
                            <field name="techpricapincre"/>
                            <field name="elrightdate"/>
                            <field name="elintdate"/>
                            <field name="curvalue"/>
                            <field name="distredyarn"/>
                            <field name="shmetbclsprc"/>
                        </group>
                        <group colspan="4" col="4">
                            <field name="shdtaxratlaw"/>
                            <field name="leglsupris"/>
                        </group>
                        <group colspan="4" col="4" string="盈餘或員工紅利轉增資">
                            <field name="etdnonslwcls"/>
                            <field name="etdslwcls"/>
                        </group>
                        <group colspan="4" col="6" string="章程訂定-%">
                            <field name="regshodiv"/>
                            <field name="regdireward"/>
                            <field name="regstafdiv"/>
                        </group>
                        <group colspan="4" col="4" string="增資緩課核備資料">
                            <field name="incapextplaname"/>
                            <field name="incapapdate"/>
                            <field name="incapapnum"/>
                            <field name="incapestfshday"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</openerp>